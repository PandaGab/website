export async function getStaticPaths() {
  const projects = await Astro.glob('../projects/*.mdx')

  return projects.map(project => ({
    params: { 
      slug: project.frontmatter.slug 
    },
    props: {
      project
    },
  }))
}
const { Content } = Astro.props.post
---
<article>
    <Content/>
</article>

